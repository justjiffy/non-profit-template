<h2>Hello World</h2>
<p>
  The time is now: <%= Time.now %>

  And the users that exist are: 
  <% @users.each do |f| %>
  <li><%= f.name %> - <%= f.email %></li>
  <% end %>

  <% @posts.each do |f| %>
  <br>
 	<h2><%= f.title %></h2>
  <%= image_tag f.img %> <span><p>
  <%= f.body %></p>
  <% end %>

</p>